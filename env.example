# Konfigurasi Server
PORT=3001

# Path untuk menyimpan sesi
SESSION_DIR=./sessions

# Timeout untuk koneksi (dalam milidetik)
CONNECTION_TIMEOUT=60000

# Jumlah maksimum percobaan reconnect
MAX_RETRIES=5

# Konfigurasi Resource (untuk shared hosting)
MAX_MEMORY_MB=150
QUEUE_PROCESS_INTERVAL=30000
MEMORY_CHECK_INTERVAL=300000

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=30

# Pengaturan Antrian
MAX_QUEUE_AGE=60000
QUEUE_CLEANUP_INTERVAL=30000
